package nomic;

import nomicGUI.UserGUI;

public abstract class User {
	
	String name;
	UserGUI screen;
	
	User(String name){
		this.name = name;
	}
	
	public String suggestion(Game game){
		// game.currentSuggestion = new Rule();
		screen.getSuggestion();
	}
	
	public void vote(Game game, Rule rule){
		
	}
}
