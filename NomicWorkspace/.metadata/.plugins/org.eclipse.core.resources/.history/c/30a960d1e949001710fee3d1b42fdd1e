package nomic;

import nomicGUI.UserGUI;

public abstract class User {
	
	String name;
	UserGUI screen;
	
	User(String name){
		this.name = name;+
	}
	
	public void takeTurn(Game game){
		suggestRule(game);
		vote();
		endTurn();
	}
	
	public void suggestRule(Game game){
		game.currentSuggestion = new Rule;
	}
	
	public void vote(Game game){
		
	}
}
